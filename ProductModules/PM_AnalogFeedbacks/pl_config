menu "PM Analog Feedbacks"
#    

depends on PM_ANALOG_FEEDBACKS_SUPPORT    
config PM_ANALOG_FEEDBACKS_NUM
    
  #depends TODO
  int "Number [1..16]"
  range 1 16
  default 1
  help
    Number of analog feedbacks


endmenu
